        <SQLTask:SqlTaskData
          SQLTask:Connection="{B731D867-5A35-4330-BBE3-B5058D2FF7CB}"
          SQLTask:SqlStatementSource="INSERT INTO dbo.[Leave]&#xA;(&#xA;  EmployeeID,&#xA;  LeaveTypeID,&#xA;  StartDate,&#xA;  EndDate,&#xA;  NumberOfDays,&#xA;  ApprovalObtained,&#xA;  SickNoteFilePath&#xA;)&#xA;SELECT&#xA;  e.EmployeeID,&#xA;  lt.LeaveTypeID,&#xA;  s.StartDate,&#xA;  s.EndDate,&#xA;  s.NumberOfDays,&#xA;  -- convert 'Yes'/'No' into a true bit&#xA;  CASE &#xA;    WHEN LOWER(s.ApprovalObtained) IN ('yes','true','1') THEN 1 &#xA;    ELSE 0 &#xA;  END AS ApprovalObtained,&#xA;  s.SickNote&#xA;FROM dbo.Stg_Leave1 AS s&#xA;INNER JOIN dbo.Employee  AS e  ON e.EmployeeName  = s.EmployeeName&#xA;INNER JOIN dbo.LeaveType AS lt ON lt.TypeName      = s.TypeOfLeave&#xA;WHERE&#xA;  s.EmployeeName    IS NOT NULL&#xA;  AND s.TypeOfLeave  IS NOT NULL&#xA;  AND s.StartDate    IS NOT NULL&#xA;  AND s.EndDate      IS NOT NULL&#xA;  AND NOT EXISTS&#xA;  (&#xA;    SELECT 1&#xA;      FROM dbo.[Leave] AS x&#xA;     WHERE x.EmployeeID    = e.EmployeeID&#xA;       AND x.LeaveTypeID   = lt.LeaveTypeID&#xA;       AND x.StartDate     = s.StartDate&#xA;       AND x.EndDate       = s.EndDate&#xA;  );" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>

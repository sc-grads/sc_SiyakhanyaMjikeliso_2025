         *  Dts.Connections["Prices.zip"].ReleaseConnection(rawConnection);
         * */
        #endregion


		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
            string excelPath = Dts.Variables["User::FullFilePath"].Value.ToString();
            string rootFolder = Dts.Variables["User::SourceRootFolder"].Value.ToString();

            // 1) Build ACE connection string
            string conn = "Provider=Microsoft.ACE.OLEDB.12.0;" +
                          "Data Source=" + excelPath + ";" +
                          "Extended Properties=\"Excel 12.0 Xml;HDR=YES;IMEX=1\";";

            Dts.Variables["User::ExcelConnString"].Value = conn;

            // 2) Detect month (Jan–Dec) in filename
            string fileName = Path.GetFileName(excelPath);
            string bareName = Path.GetFileNameWithoutExtension(excelPath);
            string[] months = { "Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec" };
            string mon = months.FirstOrDefault(m => bareName.IndexOf(m, StringComparison.OrdinalIgnoreCase) >= 0);

            // 3) Open and scan first date to compute days in month
            using (var cn = new OleDbConnection(conn))
            {
                cn.Open();
                string sheet = mon + "$";
                // Read first non-null date in A8:J10
                var cmd = new OleDbCommand($"SELECT TOP 1 [Date] FROM [{sheet}A8:J10] WHERE [Date] IS NOT NULL", cn);
                object val = cmd.ExecuteScalar();
                if (val == null) throw new Exception("No date found in " + sheet);
                DateTime dt = DateTime.Parse(val.ToString());
                int days = DateTime.DaysInMonth(dt.Year, dt.Month);
                int lastRow = 8 + days;

                // 4) Build range and store it
                string range = $"{sheet}A8:J{lastRow}";
                Dts.Variables["User::Sheets"].Value = range;
            }

            // 5) Store file & folder names
            Dts.Variables["User::FileName"].Value = Path.GetFileName(excelPath);
            Dts.Variables["User::SourceFolder"].Value = Path.GetDirectoryName(excelPath).Split('\\').Last();

            Dts.TaskResult = (int)ScriptResults.Success;
		}

